{
    "name": "sanpi/spore",
    "description": "Quickly create your next awesome PHP project based on silex, pomm, bootstrap, atoum and behat",
    "keywords": ["silex", "pomm", "bootstrap", "atoum", "behat", "skeleton"],
    "license": "MIT",
    "require": {
        "silex/silex": "1.0.*@dev",
        "pomm/pomm": "1.1.*",
        "twig/twig": "1.12.*",
        "twitter/bootstrap": "2.2.*",
        "leafo/lessphp": "0.3.*",
        "sanpi/twitter-bootstrap-installer": "*@dev"
    },
    "require-dev": {
        "silex/web-profiler": "1.0.*@dev",
        "symfony/web-profiler-bundle": "@dev",
        "symfony/stopwatch": "@dev",
        "symfony/http-kernel": "@dev",
        "symfony/routing": "@dev",
        "symfony/http-foundation": "@dev",
        "symfony/event-dispatcher": "@dev",
        "symfony/twig-bridge": "@dev",
        "atoum/atoum": "*@dev",
        "sanpi/behatch-contexts": "*@dev",
        "behat/mink-goutte-driver": "*"
    },
    "suggest": {
        "FortAwesome/Font-Awesome": "The iconic font designed for use with Twitter Bootstrap"
    },
    "scripts": {
        "post-install-cmd": "Sanpi\\TwitterBootstrap\\Composer\\ScriptHandler::postInstall",
        "post-update-cmd": "Sanpi\\TwitterBootstrap\\Composer\\ScriptHandler::postUpdate"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "symfony-web-dir": "web"
    }
}
