{
    "name": "sanpi/spore",
    "description": "Quickly create your next awesome PHP project based on silex, pomm, bootstrap, atoum and behat",
    "keywords": ["silex", "pomm", "bootstrap", "atoum", "behat", "skeleton"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "silex/silex": "~2.0",
        "pomm-project/pomm-service-provider": "dev-silex-2",
        "twig/twig": "~1.18"
    },
    "require-dev": {
        "silex/web-profiler": "~2.0",
        "pomm-project/cli": "~2.0",
        "pomm-project/pomm-profiler-service-provider": "dev-silex-2",
        "atoum/atoum": "~2.0",
        "behatch/contexts": "~2.0",
        "behat/mink-goutte-driver": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Test\\": "tests" },
        "files": ["tests/Features/bootstrap/FeatureContext.php"]
    },
    "extra": {
        "symfony-web-dir": "web"
    }
}
